<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_help_tow_xq"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#fff"
    tools:context="com.example.administrator.lenglian.fragment.mine.HelpTowXQActivity">
    <RelativeLayout
        android:background="#fff"
        android:id="@+id/rl_root_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        >
        <ImageView
            android:id="@+id/tv_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:src="@drawable/icon_back"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="帮助中心"
            android:textColor="@color/textblack"
            android:textSize="18sp"/>
    </RelativeLayout>
    <View
        android:background="#eee"
        android:layout_width="match_parent"
        android:layout_height="1dip"></View>
    <TextView
        android:textColor="@color/chengse"
        android:textSize="16sp"
        android:gravity="center_vertical"
        android:text="适配机型"
        android:paddingLeft="20dip"
        android:layout_width="match_parent"
        android:layout_height="40dip" />
    <TextView
        android:textColor="#666"
        android:textSize="16sp"
        android:gravity="center_vertical"
        android:text="        适配机型佛挡杀佛加多少了房间都是佛的数据佛顶山姐夫ID手机发的司法鉴定司法鉴定死了飞机当时开了房间都是开了房间的数量"
        android:paddingLeft="20dip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
